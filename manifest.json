{
  "manifest_version": 2,

  "name": "PageSpeed",
  "description": "Check the performance of the current page",
  "version": "0.1",

  "browser_action": {
    "default_title": "Check this page!",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "options_page": "options.html",

  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ]
}
