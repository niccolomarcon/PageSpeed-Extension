{
  "manifest_version": 2,

  "name": "PageSpeed",
  "description": "Check the performance of the current page",
  "version": "0.1",

  "browser_action": {
    "default_title": "Check this page!",
    "default_icon": "icon.png"
  },

  "background": {
    "scripts": [ "index.js" ]
  },

  "options_page": "options.html",

  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ]
}
