{
  "manifest_version": 2,

  "name": "PageSpeed",
  "description": "Check the performance of the current page",
  "version": "1.0.0",
  "icons": {
    "64": "media/icon.png"
  },

  "browser_action": {
    "default_title": "Check this page!",
    "default_icon": "media/icon.png",
    "default_popup": "popup.html"
  },

  "options_page": "options.html",

  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ]
}
