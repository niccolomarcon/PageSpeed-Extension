{
  "manifest_version": 2,

  "name": "PageSpeed",
  "description": "Check the performance of the current page",
  "version": "1.0.0",
  "icons": {
    "16": "media/icon16.png",
    "32": "media/icon32.png",
    "48": "media/icon48.png",
    "64": "media/icon64.png",
    "128": "media/icon128.png",
    "512": "media/icon512.png"
  },

  "browser_action": {
    "default_title": "Check this page!",
    "default_popup": "popup.html"
  },

  "options_page": "options.html",

  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ]
}
